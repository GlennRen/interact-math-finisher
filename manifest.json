{
  "name": "InterAct Math Finisher",
  "version": "0.2",
  "manifest_version": 2,
  "description": "This extension finishes a set of InterAct Math problems for you",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "InteractMath Finisher",
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}